    },
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "phenx/php-svg-lib": "0.3.*",
    "sabberworm/php-css-parser": "8.1.*",
    "php": "^5.5 || ^7.2",
    "typo3/cms-core": "^6.2 || ^7.6 || ^8,7 || ^9.5 || ^10.4"

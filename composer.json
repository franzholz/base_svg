{
  "name": "jambagecom/base-svg",
  "type": "typo3-cms-extension",
  "description": "A library to read, parse and export to PDF SVG files.",
  "keywords": [
    "TYPO3",
    "extension",
    "svg",
    "phpOffice"
  ],
  "support": {
    "issues": "https://github.com/franzholz/base_svg/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Fabien MÃ©nager",
      "email": "fabien.menager@gmail.com"
    },
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^11.5 || ^12.4 || ^13.4",
    "ext-mbstring": "*",
    "sabberworm/php-css-parser": "^8.4"
  },
  "repositories": [
    {
        "type": "path",
        "url": "Vendor/php-svg-lib"
    }
  ],
  "replace": {
    "typo3-ter/base-svg": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "base_svg"
    }
  }
}
